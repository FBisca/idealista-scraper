{
  "name": "@workspace/scraper",
  "version": "1.0.0",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "postinstall": "node ./scripts/ensure-ulixee-native.mjs",
    "cli": "tsx ./src/cli.ts",
    "build": "tsc",
    "dev": "tsc --watch --noEmit",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "clean": "rm -rf dist .turbo node_modules",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@ulixee/chrome-139-0": "^7258.155.11",
    "@ulixee/chrome-143-0": "^7499.193.11",
    "@ulixee/cloud": "2.0.0-alpha.34",
    "@ulixee/hero": "2.0.0-alpha.34",
    "@ulixee/real-user-agents": "2.0.0-alpha.34",
    "@ulixee/unblocked-agent-mitm-socket": "^2.0.0-alpha.34",
    "@workspace/logger": "workspace:*",
    "@workspace/ulixee-crawler": "workspace:*",
    "better-sqlite3": "^11.10.0",
    "bindings": "^1.5.0",
    "cheerio": "^1.1.2",
    "crawlee": "^3.16.0",
    "google-polyline": "^1.0.3",
    "got-scraping": "^4.1.3",
    "pdf-parse": "^2.4.5",
    "pino": "^10.2.0",
    "psl": "^1.15.0",
    "scrape-it": "^6.1.12",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/google-polyline": "^1.0.2",
    "@types/node": "^20.19.25",
    "@types/psl": "1.1.3",
    "@vitest/ui": "^2.1.8",
    "tsx": "^4.20.4",
    "typescript": "^5.9.3",
    "vitest": "^2.1.8"
  }
}
